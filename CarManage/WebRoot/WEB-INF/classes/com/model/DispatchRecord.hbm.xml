<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.model.DispatchRecord" table="DISPATCHRECORD" >
        <id name="dpid" type="java.lang.String">
            <column name="DPID" length="20" />
              <generator class="sequence">
			    <param name="sequence">DISPATCHRECORD_ID_SEQ</param>    
			  </generator>
        </id>
        <property name="cph0" type="java.lang.String">
            <column name="CPH0" length="10" />
        </property>
        <property name="sqsj" type="java.lang.String">
            <column name="SQSJ" length="20" />
        </property>
        <property name="sqyg" type="java.lang.String">
            <column name="SQYG" length="20" />
        </property>
        <property name="dpsy" type="java.lang.String">
            <column name="DPSY" length="20" />
        </property>
        <property name="dpyy" type="java.lang.String">
            <column name="DPYY" length="20" />
        </property>
        <property name="cfsj" type="java.lang.String">
            <column name="CFSJ" length="20" />
        </property>
        <property name="ddsj" type="java.lang.String">
            <column name="DDSJ" length="20" />
        </property>
    </class>
</hibernate-mapping>
