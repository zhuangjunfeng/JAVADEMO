<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.model.CarWarnning" table="CARWARNNING" >
        <id name="gjid" type="java.lang.String">
            <column name="GJID" length="20" />
            <generator class="sequence">
			    <param name="sequence">CARWARING_ID_SEQ</param>    
			  </generator>
        </id>
        <property name="cph0" type="java.lang.String">
            <column name="CPH0" />
        </property>
        <property name="jsy0" type="java.lang.String">
            <column name="JSY0" />
        </property>
        <property name="gjlx" type="java.lang.String">
            <column name="GJLX" />
        </property>
        <property name="gjsj" type="java.lang.String">
            <column name="GJSJ" />
        </property>
        <property name="gjld" type="java.lang.String">
            <column name="GJLD" />
        </property>
        <property name="ldxq" type="java.lang.String">
            <column name="LDXQ" />
        </property>
    </class>
</hibernate-mapping>
