<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.model.TripRecord" table="TRIPRECORD" >
        <id name="xcid" type="java.lang.String">
            <column name="XCID" length="20" />
              <generator class="sequence">
			    <param name="sequence">TRIPRECORD_ID_SEQ</param>    
			  </generator>
        </id>
        <property name="cph0" type="java.lang.String">
            <column name="CPH0" length="10" />
        </property>
        <property name="cksj" type="java.lang.String">
            <column name="CKSJ" length="20" />
        </property>
        <property name="gksj" type="java.lang.String">
            <column name="GKSJ" length="20" />
        </property>
        <property name="xcgj" type="java.lang.String">
            <column name="XCGJ" length="20" />
        </property>
        <property name="jsr0" type="java.lang.String">
            <column name="JSR0" length="20" />
        </property>
        <property name="sqyg" type="java.lang.String">
            <column name="SQYG" length="20" />
        </property>
    </class>
</hibernate-mapping>
