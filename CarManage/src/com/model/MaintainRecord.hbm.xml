<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.model.MaintainRecord" table="MAINTAINRECORD" >
        <id name="wbid" type="java.lang.String">
            <column name="WBID" length="20" />
             <generator class="sequence">
			    <param name="sequence">MAINTAINRECORD_ID_SEQ</param>    
			  </generator>
        </id>
        <property name="cph0" type="java.lang.String">
            <column name="CPH0" length="10" />
        </property>
        <property name="cksj" type="java.lang.String">
            <column name="CKSJ" length="20" />
        </property>
        <property name="gksj" type="java.lang.String">
            <column name="GKSJ" length="20" />
        </property>
        <property name="wbje" type="java.lang.String">
            <column name="WBJE" length="20" />
        </property>
        <property name="cardirver" type="java.lang.String">
            <column name="CARDIRVER" length="20" />
        </property>
        <property name="sqyg" type="java.lang.String">
            <column name="SQYG" length="10" />
        </property>
    </class>
</hibernate-mapping>
