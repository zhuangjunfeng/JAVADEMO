<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.model.SysUser" table="SYSUSER" >
        <id name="yhid" type="java.lang.Long">
            <column name="YHID" precision="10" scale="0" />
              <generator class="sequence">
			    <param name="sequence">SYSUSER_ID_SEQ</param>    
			  </generator>
        </id>
        <property name="yhxm" type="java.lang.String">
            <column name="YHXM" length="10" />
        </property>
        <property name="yhgs" type="java.lang.String">
            <column name="YHGS" length="20" />
        </property>
        <property name="yhyx" type="java.lang.String">
            <column name="YHYX" length="20" />
        </property>
        <property name="yhmm" type="java.lang.String">
            <column name="YHMM" length="50" />
        </property>
        <property name="yhjs" type="java.lang.String">
            <column name="YHJS" length="20" />
        </property>
        <property name="yhzh" type="java.lang.String">
            <column name="YHZH" length="20" />
        </property>
        <property name="sjhm" type="java.lang.Integer">
            <column name="SJHM" precision="20" scale="0" />
        </property>
        <property name="yhxb" type="java.lang.String">
            <column name="YHXB" length="20" />
        </property>
    </class>
</hibernate-mapping>
