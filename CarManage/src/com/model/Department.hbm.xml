<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.model.Department" table="DEPARTMENT" >
        <id name="bmid" type="java.lang.String">
            <column name="BMID" length="20" />
              <generator class="sequence">
			    <param name="sequence">DEPARTMENT_ID_SEQ</param>    
			  </generator>
        </id>
        <property name="bmmc" type="java.lang.String">
            <column name="BMMC" length="20" />
        </property>
        <property name="bmms" type="java.lang.String">
            <column name="BMMS" length="100" />
        </property>
        <property name="bmqx" type="java.lang.String">
            <column name="BMQX" length="20" />
        </property>
        <property name="ssjg" type="java.lang.String">
            <column name="SSJG" length="20" />
        </property>
    </class>
</hibernate-mapping>
